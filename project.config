[project]
	description = Simple game for university project
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/mkulagowski
[access "refs/for/refs/*"]
	read = group user/mkulagowski
	push = group user/mkulagowski
	label-Code-Review = -2..+2 group user/mkulagowski
	label-Verified = -1..+1 group user/mkulagowski
	rebase = group user/mkulagowski
	removeReviewer = group user/mkulagowski
	submit = group user/mkulagowski
[access "refs/heads/*"]
	read = group user/mkulagowski
	create = group user/mkulagowski
	push = +force group user/mkulagowski
	pushMerge = group user/mkulagowski
[access "refs/tags/*"]
	push = +force group user/mkulagowski
	pushTag = group user/mkulagowski
	pushSignedTag = group user/mkulagowski
